{
  "version": 2,
  "name": "clearpath-ai",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "functions": {
    "frontend/app/**": {
      "includeFiles": "frontend/**"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
