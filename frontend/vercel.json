{
  "version": 2,
  "name": "clearpath-ai",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "app/**/*.tsx": {
      "maxDuration": 10
    },
    "app/**/*.ts": {
      "maxDuration": 10
    }
  }
}
